{
  "name": "Github PR vendored files remover",
  "version": "0.1",
  "description": "Exclude vendor/ prefixed files from  Github files change view",
  "permissions": [
    "activeTab", "scripting"
  ],
  "background": {
    "service_worker": "./lib/js/background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/gopher16.png",
      "32": "images/gopher32.png",
      "48": "images/gopher48.png",
      "128": "images/gopher128.png"
    }
  },
  "icons": {
    "16": "images/gopher16.png",
    "32": "images/gopher32.png",
    "48": "images/gopher48.png",
    "128": "images/gopher128.png"
  },
  "manifest_version": 3
}
